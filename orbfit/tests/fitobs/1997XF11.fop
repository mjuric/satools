! input file for fitobs
fitobs.
! first arc
        .astna0='1997 XF11'    ! full name, first arc
        .obsdir0='obsdata'    ! directory of observs. file, first arc 
        .elefi0='astorb.cut' ! first arc elements file

! second arc
        .astnap='1997 XF11pre'    ! full name, second arc
        .obsdirp='obsdata'    ! directory of observs. file, second arc 
        .namp='helin'       ! name of second arc asteroid obs.file

! xephem integration
        .ixeph= 0            ! xephem integration level, 0=no
